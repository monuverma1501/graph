#include<bits/stdc++.h>
using namespace std;
#define local 
#define ll long int

void bfs(int node,vector<int> adj[],vector<int> &vis){
    queue<int> q;
    q.push(node);
    vis[node]=1;
    while(!q.empty())
    {
        int x=q.front();
        q.pop();
        cout<<x<<" ";
        for(auto it:adj[x]){
            if(!vis[it])
            {
                q.push(it);
                vis[it]=1;
            }
        }
    }
     
}

int main(){
	int n,m;
	cin>>n>>m;
	vector<int> adj[n+1];
	for(int i=0;i<m;i++)
	{
		int u,v;
		cin>>u>>v;
		adj[u].push_back(v);
		adj[v].push_back(u);
	}
	vector<int> vis(n+1,0);
	for(int i=1;i<=n;i++){
	    if(!vis[i])
	    {
    	    bfs(i,adj,vis);
	    }
	}
}